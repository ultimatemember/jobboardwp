{
   "name": "ultimatemember/jobboardwp",
   "description": "JobBoardWP tools for development",
   "type": "wordpress-plugin",
   "homepage": "https://wordpress.org/plugins/jobboardwp/",
   "license": "GPL-3.0-or-later",
   "authors": [
      {
         "name": "Ultimate Member Ltd.",
         "email": "team@ultimatemember.com"
      }
   ],
   "minimum-stability": "stable",
   "prefer-stable" : true,
   "config": {
      "allow-plugins": {
         "symfony/flex": true,
         "dealerdirect/phpcodesniffer-composer-installer": true
      }
   },
   "require": {
      "ext-json": "*",
      "ext-dom": "*",
      "ext-exif": "*",
      "ext-gd": "*",
      "ext-fileinfo": "*",
      "ext-curl": "*",
      "ext-iconv": "*"
   },
   "scripts": {
   },
   "require-dev": {
     "wp-cli/wp-cli": "*",
     "dealerdirect/phpcodesniffer-composer-installer": "*",
     "phpcompatibility/phpcompatibility-wp": "*",
     "wp-coding-standards/wpcs": "*",
     "symplify/phpstan-rules": "*",
     "phpdocumentor/phpdocumentor": "3.8.*"
   }
}
